{
  "extends": [
    "config:recommended",
    "config:base"
  ],
  "enabledManagers": [
    "cargo",
    "helmv3"
  ],
  "packageRules": [
    {
      "matchDatasources": [
        "cargo"
      ],
      "matchPackageNames": [
        "*"
      ],
      "osvVulnerabilityAlerts": true,
      "automerge": false,
      "labels": [
        "rust",
        "vulnerability"
      ]
    },
    {
      "matchManagers": [
        "helmv3"
      ],
      "matchPackageNames": [
        "deprecated-chart"
      ],
      "enabled": true
    }
  ],
  "helmv3": {
    "fileMatch": [
      "Chart.yaml"
    ],
    "enabled": true
  },
  "osvVulnerabilityAlerts": true,
  "dependencyDashboard": true,
  "schedule": [
    "every weekday after 9am and before 5pm"
  ],
  "rollbackPrs": true
}
